
<div class='search-background'>
  <div class='search'>
    <h1> OpenClinical</h1>
    <h4>Search Public Clinical Trial Data</h4>
    {{input value=model.filter.officialTitle placeholder='Search By Title'}}
    {{#power-select-multiple options=model.phaseOptions selected=model.filter.selectedPhases placeholder="All Phases" onchange=(action (mut model.filter.selectedPhases )) as |phase|}}
      {{phase}}
    {{/power-select-multiple}}
    <button class='btn btn-primary' {{action 'searchStudies' }}> Search </button>
      {{search}}
    </div>
</div>
